{
    "files.exclude": {
        "**/.vagrant": true,
        "venv": true,
        "*.lock": true
    },
    "files.associations": {
        "**/*.yml": "ansible"
    },
    "python.pythonPath": ".venv/bin/python",
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/.venv/lib/python3.9",
        "${workspaceFolder}/.venv/lib/python3.9/site-packages"
    ],
    "python.analysis.extraPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/.venv/lib/python3.9",
        "${workspaceFolder}/.venv/lib/python3.9/site-packages"
    ]
}
